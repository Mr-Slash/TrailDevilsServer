<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	
	<managed-bean>
		<managed-bean-name>trailsModel</managed-bean-name>
		<managed-bean-class>ch.hsr.traildevils.viewer.TrailsModel</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>trailsController</managed-bean-name>
		<managed-bean-class>ch.hsr.traildevils.viewer.TrailsController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>syncTrailController</managed-bean-name>
		<managed-bean-class>ch.hsr.traildevils.synchronize.SyncTrailController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

</faces-config>
